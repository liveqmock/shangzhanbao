#AdLayer {
	position:absolute;
	top:0px;
	right:0px;
	width:20px;
	display:none;
	height:59px;
}
img {
	border:0;
}
.Header {
	position:fixed;
	top:0;
	z-index:100;
	padding:0;
	width:100%;
	height:auto;
	background:#ffffff;
	border-bottom:10px solid #efefef;
}
#logo {
	float: left;
	margin :15px 2px 10px 10px;
}
#nav {
	float:right;
	margin:50px 0 0 0;
}
.navigation {
	float:right;
	list-style:none;
	margin:0;
	color: #233746;
}
.navigation li {
	float: left;
	padding:0 0 0 25px;
	font-size: 1em;
	text-align: center;
	font-weight: 300;
	line-height:35px;
	color: #233746;
	cursor:pointer;
	text-decoration:none
}
.navigation li a {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
}
.navigation li a:hover {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
}
.navigation a:active {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
}
/*Wrap*/
.wraper {
	width: 100%;
	margin: 0 auto;
}
#sidebar {
	width:100%;
}
.container {
	width: 98%; /*width: 1000px;*/
	max-width: 1000px;
	margin:0 auto;
	clear:both
}
.slide {
	background-attachment:fixed;
	width:100%;
	position:relative;
	padding:180px 0 0 0;
	margin:0;
	
}
.Btn_inputbtn{
	background:url(../images/frame_btnbg.png) no-repeat -111px -32px blue;
	width:110px;
	height:30px;
	line-height:30px;
