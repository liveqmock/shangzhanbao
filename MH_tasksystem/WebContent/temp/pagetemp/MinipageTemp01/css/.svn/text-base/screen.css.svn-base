}
/*banner Styles *********************************/
#Banerlider {
	width: 100%;
	margin:0;
	padding: 0;
	border-bottom:5px solid #89a2b2;
	background:#444a4b;
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
	 filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/feature.jpg', sizingMethod='scale');
	-ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/feature.jpg', sizingMethod='scale')";
	padding:200px 0 0 0;
	color:#fff
}
#Banerlider .BanerText {
	float:left;
	margin:0 10px 10px 0px;
	-webkit-backface-visibility: hidden;
	color:#fff;
}
.bg1{
	background:#353945;
	color:#d2dadc;
}
.bg2{
	background:#929699;
	color:#343738;
}
.bg3{
	background:#d2984e;
	color:#6d470b;
}
	
#Banerlider .BanerText1{
	font-size:12px;
	width:30%;
	float:left;
	border-radius:5px;
	-o-border-radius:5px;
	-web-border-radius:5px;
	margin:10px 5px 10px 0;
}
#Banerlider .BanerText1 ul li{
	padding:0;
	margin:0;
	list-style:none;
}
#Banerlider .BanerText1 li{
	height:30px;
	line-height:30px;
	text-align:left;
	text-indent:8px
}


/* Hide the slides before the JS is loaded. Avoids image jumping */
#Banerlider img {
	display: block;
	float:right;
	max-width:100%;
}
.BanerText h2 {
	font:400 32px/1.2 "microsoft yahei", Tahoma, arial, sans-serif;
	color: #fff;
	margin: 20px 0 16px;
}
.BanerText p {
	margin: 0 0 10px 0;
}
.BanerText .money{
	font-size: 1em;
	line-height: 23px;
	padding-right: 25px;
	color:#fff;
	float:left;
	padding-bottom:20px
	}
	
.copy {
	font-size: 12px;
	line-height:30px;
	padding-right: 15px;
	color:#9aa1a2;
	
}
