/** click_a  **/
.click_a{background-color:#e6ffbd;color:#ff6600; text-decoration:underline}
.txt_box_groupUser{width:80px;height:18px;padding:3px 0 0 3px;border:1px solid #028300;}
.txt_box_groupBase{width:170px;height:18px;padding:3px 0 0 3px;border:1px solid #028300;}