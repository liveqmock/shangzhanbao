/* 全局样式 */
*{margin:0;padding:0;font-size:12px;font-family:"宋体";}
body{background:#fff;color:#000;line-height:18px}
table{ border-collapse:collapse}
input{vertical-align:middle; }
a:link,a:visited{text-decoration:none;color:#3F6C8B;}
a:hover, a:active, a:focus { color:#FF0000;}
.clear {clear:both;font-size:0;line-height:0 }

/** */
table.main_tab{width:100%;}
/*table.main_tab{width:800px;}*/
.main_tab td{padding:3px 6px;border:1px solid #000;}
.main_tab .th_bg{height:18px;padding:2px 6px;background:url(../images/tab_th_bg.gif) repeat-x top;border-right:1px solid #000;color:#fff;font-weight:normal;}

table.no_br{width:100%}
.no_br td{border:0;padding:6px;}
.no_br .font_siz h3{height:19px;padding:2px 6px;background:url(../images/tab_th_bg.gif) repeat-x top;border-bottom:1px solid #000;color:#fff;font-weight:bold;font-size:12px;text-align:center;}
.no_br .font_siz h4{height:21px;padding:3px 0 0 12px;font-weight:normal;}
.main_tab .bgcol {background-color:#ecf5e4;}
/*****/
.btn_min{width:55px;height:29px;background:url(../images/btn_min.gif) no-repeat left top;border:0 none;color:#fff;}
.btn2{width:68px;height:30px;background:url(../images/btn_2.gif) no-repeat left top;border:0 none;color:#fff;}
.txt_box{width:130px;height:18px;padding:3px 0 0 3px;border:1px solid #028300;}
.btnBg {
	FONT-SIZE: 13px;
	font-weight:bold;
	width:85px;
	height:32px;
	line-height:32px;
	border:0;
	background:url(../images/btnBg.gif);
	color:#656565;
}

.sel{background-color:#ecf5e4;text-decoration:underline;}
.sel a:hover{background-color:#d5f6b8;}
