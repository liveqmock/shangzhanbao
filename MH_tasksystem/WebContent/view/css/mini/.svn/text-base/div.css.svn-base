.DataStatistics_1_CL {
	width:200px;
	float:left;
	margin:0 0 15px 25px;
	overflow:hidden;
}
.DataStatistics_1_CL ul {
	margin:0;
	padding:0;
}
.DataStatistics_1_CL li {
	width:200px;
	float:left;
	list-style:none;
	color:#0099ff;
	font-size:14px;
	line-height:30px;
	margin:15px 0 10px 0;
}
.DataStatistics_1_CL li a {
	width:200px;
	float:left;
}
.DataStatistics_1_CL li span {
	float:left;
	color:#666;
	font-size:26pt;
	font-family:Arial, Helvetica, sans-serif;
	margin:5px 0 0 0;
	width:120px;
}
.DataStatistics_1_CL li p {
	float:left;
	width:200px;
	margin:0;
	padding:0;
	overflow:hidden
}
.DataStatistics_1_CL li p font {
	float:right;
	margin:0 0 0 10px;
	width:76px;
	cursor:pointer;
}
.DataStatistics_1_CL li font.top {
	width:20px;
	height:35px;
	float:left;
	overflow:hidden;
	background:url(../images/line.png) no-repeat 0 0;
}
.DataStatistics_1_CL li font.bottom {
	width:20px;
	height:35px;
	float:left;
	overflow:hidden;
	background:url(../images/line.png) no-repeat -20px 0;
}
.DataStatistics_1_CL li font.center {
	width:19px;
	height:8px;
	float:left;
	overflow:hidden;
	background:url(../images/line.png) no-repeat 0 -37px;
	margin:18px 0 0 10px;
}
