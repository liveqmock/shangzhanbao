}
.logo {
	float: left;
	margin :15px 2px 0px 10px;
}
.logo img{
	height:70px;
	margin-top: -20px;
	min-width: 80px;
}
.nav {
	float:right;
	margin:20px 0 0 0;
}
.navigate {
	float:right;
	list-style:none;
	margin:0;
	color: #233746;
	margin-bottom: 10px;
}
.navigate li {
	float: left;
	padding:0 0 0 25px;
	font-size: 1em;
	font-weight: 300;
	/* line-height:35px; */
	color: white;
	text-decoration:none
}
.navigate li a {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
	-webkit-tap-highlight-color:rgba(255,0,0,0);
}
.navigate li a:hover {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
}
.navigate a:active {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
}
