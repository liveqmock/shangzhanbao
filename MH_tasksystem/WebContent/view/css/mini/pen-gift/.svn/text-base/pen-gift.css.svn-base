/* CSS Document */

h1 {
	font-size: 48px;
	margin-bottom: 10px;
}
h2 {
	font-size: 20px;
	font-weight: 600;
	text-transform: uppercase;
}
h2 {
	margin:25px 0 25px 0;
}
h3 {
font-size: 22px;
line-height: 32px;
color: #971100;
margin-bottom: 15px;
text-transform: uppercase;
}
h4 {
	font-size: 14px;
	margin-bottom: 10px;
}
h5 {
	font-size: 12px;
	margin-bottom: 10px;
}
h6 {
	font-size: 11px;
	margin-bottom: 10px;
}
.Btn_Td{
	background: url(../../../pages/mini/page/images/frame_btnbg.png) no-repeat -111px -32px;
	width:110px;
	height:30px;
	line-height:30px;
	text-align:center;
	font-size:12px;
	display:block;
	color:#fff;
	overflow:hidden;
	text-decoration:none;
	border:none;
	clear:both;
}

<!--返回顶部-->
.main {width:1000px;height:4800px;margin:0 auto;}
#AdLayer {position:absolute;top:0px;right:0px;width:20px;display:none;height:59px;}
img {border:0;}

/*导航*/
#daohang {
	position:fixed;
	top:0;
	z-index:100;
	padding:0;
	width:100%;
	height:auto;
	background:#ffffff;
	border-bottom:10px solid #efefef;
}
.logo {
	float: left; 
	margin :15px 2px px 10px;
}
.logo img{
	height:70px;
	margin-top: -20px;
	float: left;
	min-width: 80px;
	margin: 0 auto;
}
.nav {
	float:right;
	margin:20px 0 0 0;
}
.navigate {
	float:right;
	list-style:none;
	margin:0;
	color: #233746;
	margin-bottom: 10px;
}
.navigate li {
	float: left;
	padding:0 0 0 25px;
	font-size: 1em;
	font-weight: 300;
	/* line-height:35px; */
	color: white;
	text-decoration:none
}
.navigate li a {
	cursor:pointer;
	color: #233746;
	text-decoration:none;
	-webkit-tap-highlight-color:rgba(255,0,0,0);
}

/*Wrap*/
.wraper {
	width: 100%;
	margin: 0 auto;
}
/*增加侯杨*/
.wraper h2{
	width: 98%;
	margin:20px 0px 10px 0;
	font-size: 22px;
	font-weight: bold;
	text-transform: uppercase;
}
#phone{
	max-width: 70%;
	float: right;
	width: 70%;
/* 	margin-left: 70%; */
}
#sidebar {
	width:100%;
}
.container {
	width: 98%; /*width: 1000px;*/
	max-width: 1000px;
	margin:0 auto;
	clear:both
}

/*banner Styles *********************************/
#Banerlider {
	width: 100%;
	margin:0;
	padding: 0;
	border-bottom:5px solid #89a2b2;
/* 	background: url(../images/bannerbg.jpg) #aeaeae; */
	-webkit-background-size: cover;
	-moz-background-size: cover;
	-o-background-size: cover;
	background-size: cover;
 	filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/feature.jpg', sizingMethod='scale');
	-ms-filter: "progid:DXImageTransform.Microsoft.AlphaImageLoader(src='../images/feature.jpg', sizingMethod='scale')";
	padding-top:140px;
	padding-bottom:45px;
	margin: 0 2% 1% 0;
	margin-bottom: 0px;
}
#Banerlider .BanerText {
float: left;
margin: 0 10px 0 10px;
color: #4d4d4d;
width: 50%;
} 
/* Hide the slides before the JS is loaded. Avoids image jumping */
#Banerlider img {
display: block;
float: left;
max-width: 100%;
max-height: 500px;
overflow: hidden;
}
#banner_imgDiv{
	width: 46%;
	float: left;
	max-height: 500px;
}
.BanerText h2 {
	font:400 32px/1.2 "microsoft yahei", Tahoma, arial, sans-serif;
	color: #4d4d4d;
	margin: 20px 0 26px;
}
.BanerText p {
	margin: 0 0 30px;
}
.copy {
	font-size: 16px;
	line-height: 23px;
	padding-right: 15px;
	color:#4d4d4d;
}

.btn {
	width:133px;
	height:34px;
	line-height:34px;
	text-align:left;
	color:#fff;
	font-size:14px;
	background:url(../images/btn02.png);
	display:block;
	float:lef;
	text-indent:55px;
	cursor:pointer
}
/*共用**************sepContainer  修改侯杨********************/
