/*slide7 Styles **************产品购买*******************/

#slide7 {
	margin-bottom:40px;
}
.buybox {
	border:1px solid #dadada;
	border-radius:5px;
	-web-border-radius:15px;
	-moz-border-radius:5px;
	-o-border-radius:5px;
	background:#f4eeee;
	width:100%;
	border:1px solid #dadada;
	color:#333;
	margin-right:16px;
	padding:20px 0 0;
	text-align:center;
}
.buybox h3 {
	font-size:20px;
	font-weight:400;
	margin: 0 0 15px;
}
.buybox .price {
	display:block;
	font-size:16px;
	margin-bottom:5px
}
.buybox .price span {
	font-size:14px
}
.buybox a.flt-btn-orng {
	font-weight: 600;
	font-size: 16px;
	margin-top:9px;
	-webkit-backface-visibility:hidden
}
.flt-btn {
	background:#aaa;
	border:0;
	border-bottom:3px solid #888;
	color:#fff;
	cursor:pointer;
	display:inline-block;
	font-size:14px;
	line-height:14px;
	padding:13px 35px 11px;
	text-align:center;
	text-decoration:none
}
.flt-btn:hover {
	background:#888;
	border-color:#666
}
.flt-btn:active {
	background:#888;
	border-color:#888
}
.flt-btn-orng {
	background:#ff8400;
	border-bottom:3px solid #f06d00
}
.flt-btn-orng:hover {
	background:#ffa133;
	border-color:#ff7e12
}
.flt-btn-orng:active {
	background:#f06d00;
	border-color:#f06d00
}
.details ul {
	margin-top:20px;
	font-size:14px
}
.details li {
	border-top:1px solid #dadada;
	color:#333;
	padding:25px  0;
}
.details li strong{
	margin-right:3px;
}
 #phone a{color: #971100;text-decoration: none;}
