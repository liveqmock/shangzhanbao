package com.itour.etip.pub.kit.system;


/**
 * <b>描述: 系统CPU使用情况, 目前收集了物理,虚拟内存的使用情况</b><br>
 * <br>
 * <b>示例:</b><br>
 * <br>
 * 
 * @author zhangjj
 * @version 1.00 created on 2008-1-22
 */
public class MemoryUsageBean {
    // 全部物理内存
    private long totalPhysicalMemory = -1;
    // 已使用的物理内存
    private long availPhysicalMemory = -1;
    // 物理内存的使用率
    private int physicalMemoryUsage = -1;

    // 全部虚拟内存
    private long totalVirtualMemory = -1;
    // 已使用的虚拟内存
    private long availVirtualMemory = -1;
    // 虚拟内存的使用率
    private int virtualMemoryUsage = -1;

    /**
     * get totalPhysicalMemory
     * 
     * @return totalPhysicalMemory author zhangjj created on 2008-1-22
     */
    public long getTotalPhysicalMemory() {
        return totalPhysicalMemory;
    }

    /**
     * set totalPhysicalMemory
     * 
     * @param totalPhysicalMemory
     *            the totalPhysicalMemory to set author zhangjj created on
     *            2008-1-22
     */
    public void setTotalPhysicalMemory(long totalPhysicalMemory) {
        this.totalPhysicalMemory = totalPhysicalMemory;
    }

    /**
     * get availPhysicalMemory
     * 
     * @return availPhysicalMemory author zhangjj created on 2008-1-22
     */
    public long getAvailPhysicalMemory() {
        return availPhysicalMemory;
    }

    /**
     * set availPhysicalMemory
     * 
     * @param availPhysicalMemory
     *            the availPhysicalMemory to set author zhangjj created on
     *            2008-1-22
     */
    public void setAvailPhysicalMemory(long availPhysicalMemory) {
        this.availPhysicalMemory = availPhysicalMemory;
    }

    /**
     * get physicalMemoryUsage
     * 
     * @return physicalMemoryUsage author zhangjj created on 2008-1-22
     */
    public int getPhysicalMemoryUsage() {
        return physicalMemoryUsage;
    }

    /**
     * set physicalMemoryUsage
     * 
     * @param physicalMemoryUsage
     *            the physicalMemoryUsage to set author zhangjj created on
     *            2008-1-22
     */
    public void setPhysicalMemoryUsage(int physicalMemoryUsage) {
        this.physicalMemoryUsage = physicalMemoryUsage;
    }

    /**
     * get totalVirtualMemory
     * 
     * @return totalVirtualMemory author zhangjj created on 2008-1-22
     */
    public long getTotalVirtualMemory() {
        return totalVirtualMemory;
    }

    /**
     * set totalVirtualMemory
     * 
     * @param totalVirtualMemory
     *            the totalVirtualMemory to set author zhangjj created on
     *            2008-1-22
     */
    public void setTotalVirtualMemory(long totalVirtualMemory) {
        this.totalVirtualMemory = totalVirtualMemory;
    }

    /**
     * get availVirtualMemory
     * 
     * @return availVirtualMemory author zhangjj created on 2008-1-22
     */
    public long getAvailVirtualMemory() {
        return availVirtualMemory;
    }

    /**
     * set availVirtualMemory
     * 
     * @param availVirtualMemory
     *            the availVirtualMemory to set author zhangjj created on
     *            2008-1-22
     */
    public void setAvailVirtualMemory(long availVirtualMemory) {
        this.availVirtualMemory = availVirtualMemory;
    }

    /**
     * get virtualMemoryUsage
     * 
     * @return virtualMemoryUsage author zhangjj created on 2008-1-22
     */
    public int getVirtualMemoryUsage() {
        return virtualMemoryUsage;
    }

    /**
     * set virtualMemoryUsage
     * 
     * @param virtualMemoryUsage
     *            the virtualMemoryUsage to set author zhangjj created on
     *            2008-1-22
     */
    public void setVirtualMemoryUsage(int virtualMemoryUsage) {
        this.virtualMemoryUsage = virtualMemoryUsage;
    }

}
