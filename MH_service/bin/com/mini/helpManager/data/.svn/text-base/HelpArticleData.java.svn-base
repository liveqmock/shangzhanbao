package com.mini.helpManager.data;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Table;

import com.itour.etip.pub.frame.FrmData;
/**
 * 
 * 〈帮助与支持内容管理
 *
 * @author 冯鑫
 * @see [相关类/方法]（可选）
 * @since [产品/模块版本] （可选）
 */
@Entity
@Table(name = "MINI_HELP_ARTICLE")
public class HelpArticleData extends FrmData {

    /**
     */
    private static final long serialVersionUID = -6321091460960057169L;
    /**
     * 帮助与支持文章名称
     */
    private String helparticlename;
    /**
     * 帮助与支持文章类别
     */
    private String helparticlecate;
    /**
     * 帮助与支持文章状态
     */
    private Integer helparticlestate;
    /**
     * 帮助与支持文章 是否删除 0:删除  1：正常数据
     */
    private Integer isdelete;
    /**
     * 帮助与支持文章路径
     */
    private String helparticlepath;
    /**
     * 帮助与支持文章排序
     */
    private String helparticlesort;
    /**
     * 帮助与支持文章点击数
     */
    private Integer helparticleclicknum;
    
    
    @Column(name = "HELPARTICLENAME")
    public String getHelparticlename() {
        return helparticlename;
    }
    public void setHelparticlename(String helparticlename) {
        this.helparticlename = helparticlename;
    }
    @Column(name = "HELPARTICLECATE")
    public String getHelparticlecate() {
        return helparticlecate;
    }
    public void setHelparticlecate(String helparticlecate) {
        this.helparticlecate = helparticlecate;
    }
    @Column(name = "HELPARTICLESTATE")
    public Integer getHelparticlestate() {
        return helparticlestate;
    }
    public void setHelparticlestate(Integer helparticlestate) {
        this.helparticlestate = helparticlestate;
    }
    @Column(name = "ISDELETE")
    public Integer getIsdelete() {
        return isdelete;
    }
    public void setIsdelete(Integer isdelete) {
        this.isdelete = isdelete;
    }
    @Column(name = "HELPARTICLEPATH")
    public String getHelparticlepath() {
        return helparticlepath;
    }
    public void setHelparticlepath(String helparticlepath) {
        this.helparticlepath = helparticlepath;
    }
    @Column(name = "HELPARTICLESORT")
    public String getHelparticlesort() {
        return helparticlesort;
    }
    public void setHelparticlesort(String helparticlesort) {
        this.helparticlesort = helparticlesort;
    }
    @Column(name = "HELPARTICLECLICKNUM")
    public Integer getHelparticleclicknum() {
        return helparticleclicknum;
    }
    public void setHelparticleclicknum(Integer helparticleclicknum) {
        this.helparticleclicknum = helparticleclicknum;
    }
}
